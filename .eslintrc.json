{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "no-console": "off",
    "no-underscore-dangle": "off"
  }
}
